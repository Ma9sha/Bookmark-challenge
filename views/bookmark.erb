
<ul>
<% @bookmark.each do |bookmark|%>
  <li>
  <a href="<%= bookmark['url'] %>">  <%= bookmark['title'] %>
  </a>
  </li>
  <% end%>
  </ul>

  <div>
  <form action='/bookmarks' method="POST">
  <input type="url" name="urlname">
  <input type="text" name="title">
  <input type="submit" value="Add">
  </form>
  </div>
  
